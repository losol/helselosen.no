        <pageSimple id="1209" key="cf07aa6b-9839-4ea8-b4e7-8cb21829688c" parentID="1208" level="4" creatorID="0" sortOrder="0" createDate="2016-02-28T17:05:54" updateDate="2016-02-28T17:05:54" nodeName="Diabetes" urlName="diabetes" path="-1,1052,1087,1208,1209" isDoc="" nodeType="1085" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1084" nodeTypeAlias="pageSimple">
          <doNotShowFeaturedImage>0</doNotShowFeaturedImage>
