    <pageSection id="1121" key="67818a1f-f8c2-48b5-bf61-71216b3a7c19" parentID="1052" level="2" creatorID="0" sortOrder="2" createDate="2016-02-22T10:24:37" updateDate="2016-03-01T23:00:31" nodeName="Familie og barn" urlName="familie-og-barn" path="-1,1052,1121" isDoc="" nodeType="1239" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1238" nodeTypeAlias="pageSection">
