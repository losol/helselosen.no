        <pageSection id="1195" key="a4093790-986f-4f95-bd3f-9f33c887a0bd" parentID="1194" level="4" creatorID="0" sortOrder="0" createDate="2016-02-28T16:11:18" updateDate="2016-03-13T22:14:36" nodeName="Demens" urlName="demens" path="-1,1052,1087,1194,1195" isDoc="" nodeType="1239" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1238" nodeTypeAlias="pageSection">
