      <pageSection id="1213" key="e0a3d752-55ab-4af2-a995-f04139fac776" parentID="1125" level="3" creatorID="0" sortOrder="1" createDate="2016-02-28T17:11:32" updateDate="2016-03-02T14:05:03" nodeName="Trygdemedisin" urlName="trygdemedisin" path="-1,1052,1125,1213" isDoc="" nodeType="1239" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1238" nodeTypeAlias="pageSection">
