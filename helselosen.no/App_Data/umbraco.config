    <pagelist id="1120" key="6b14557c-98f9-4125-9cfe-d8f9c1e4de0c" parentID="1052" level="2" creatorID="0" sortOrder="10" createDate="2016-02-22T10:14:27" updateDate="2016-02-28T20:33:52" nodeName="Magasin" urlName="magasin" path="-1,1052,1120" isDoc="" nodeType="1111" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1110" nodeTypeAlias="pagelist">
