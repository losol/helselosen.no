      <pageSimple id="1179" key="f7eec393-43dc-4b63-b674-3ec6e984fbeb" parentID="1178" level="3" creatorID="0" sortOrder="0" createDate="2016-02-24T18:16:08" updateDate="2016-02-24T18:16:08" nodeName="Søvn" urlName="soevn" path="-1,1052,1178,1179" isDoc="" nodeType="1085" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1084" nodeTypeAlias="pageSimple">
