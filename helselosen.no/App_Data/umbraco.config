      <pageSimple id="1223" key="68e23cb7-ae08-47dc-8c3c-bc05e903e104" parentID="1086" level="3" creatorID="0" sortOrder="0" createDate="2016-02-28T17:37:36" updateDate="2016-02-28T17:37:36" nodeName="Forebygging av selvmord" urlName="forebygging-av-selvmord" path="-1,1052,1086,1223" isDoc="" nodeType="1085" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1084" nodeTypeAlias="pageSimple">
