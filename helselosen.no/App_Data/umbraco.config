      <pageSection id="1124" key="c0b25357-9c3d-4e43-99d6-544f12680882" parentID="1123" level="3" creatorID="0" sortOrder="0" createDate="2016-02-22T10:30:26" updateDate="2016-03-15T18:15:21" nodeName="Vitaminer" urlName="vitaminer" path="-1,1052,1123,1124" isDoc="" nodeType="1239" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1238" nodeTypeAlias="pageSection">
