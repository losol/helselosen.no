      <pageStandard id="1133" key="6e28afaa-2785-417b-aa26-7ec8a54378d3" parentID="1119" level="3" creatorID="0" sortOrder="3" createDate="2016-02-23T15:58:44" updateDate="2016-03-14T23:46:24" nodeName="Paracetamol fortsatt førstevalget ved smerter" urlName="paracetamol-fortsatt-foerstevalget-ved-smerter" path="-1,1052,1119,1133" isDoc="" nodeType="1241" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1084" nodeTypeAlias="pageStandard">
