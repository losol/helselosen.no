      <pageSection id="1126" key="dfc87dde-dfbc-4c3f-a383-357d953b3e47" parentID="1125" level="3" creatorID="0" sortOrder="0" createDate="2016-02-22T10:32:14" updateDate="2016-03-02T14:04:53" nodeName="Førerkort" urlName="foererkort" path="-1,1052,1125,1126" isDoc="" nodeType="1239" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1238" nodeTypeAlias="pageSection">
        <doNotShowFeaturedImage>0</doNotShowFeaturedImage>
