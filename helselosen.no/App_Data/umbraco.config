      <pageMagazine id="1152" key="69e7944f-e76b-486b-ac38-1eaebe23e269" parentID="1120" level="3" creatorID="0" sortOrder="3" createDate="2016-02-23T22:39:45" updateDate="2016-02-23T22:54:29" nodeName="Fremdeles den samme - selv med demens" urlName="fremdeles-den-samme-selv-med-demens" path="-1,1052,1120,1152" isDoc="" nodeType="1140" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1139" nodeTypeAlias="pageMagazine">
