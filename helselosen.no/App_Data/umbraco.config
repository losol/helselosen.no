      <pageSimple id="1170" key="1f9b130f-e946-4edf-b024-cee5826c039d" parentID="1121" level="3" creatorID="0" sortOrder="0" createDate="2016-02-24T16:32:46" updateDate="2016-03-03T22:00:38" nodeName="Råd om nettbrett og småbarn" urlName="raad-om-nettbrett-og-smaabarn" path="-1,1052,1121,1170" isDoc="" nodeType="1085" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1084" nodeTypeAlias="pageSimple">
