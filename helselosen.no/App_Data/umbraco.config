        <pageSimple id="1218" key="2df6ad9f-ae8e-45e7-832a-38a3fe345507" parentID="1126" level="4" creatorID="0" sortOrder="1" createDate="2016-02-28T17:19:44" updateDate="2016-03-03T22:01:45" nodeName="Vanedannende legemidler og førerkort" urlName="vanedannende-legemidler-og-foererkort" path="-1,1052,1125,1126,1218" isDoc="" nodeType="1085" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1084" nodeTypeAlias="pageSimple">
