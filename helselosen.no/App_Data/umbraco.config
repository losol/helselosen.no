      <pageMagazine id="1154" key="d17ee82a-3dd6-474d-a9f6-374ef704de58" parentID="1120" level="3" creatorID="0" sortOrder="5" createDate="2016-02-23T23:00:16" updateDate="2016-02-23T23:01:25" nodeName="Forhåndssamtaler om døden" urlName="forhaandssamtaler-om-doeden" path="-1,1052,1120,1154" isDoc="" nodeType="1140" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1139" nodeTypeAlias="pageMagazine">
