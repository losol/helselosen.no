    <pageSection id="1125" key="fb72bbf2-7994-4c4e-beea-6d57a3184896" parentID="1052" level="2" creatorID="0" sortOrder="9" createDate="2016-02-22T10:31:29" updateDate="2016-03-02T14:04:43" nodeName="Lov og rett" urlName="lov-og-rett" path="-1,1052,1125" isDoc="" nodeType="1239" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1238" nodeTypeAlias="pageSection">
