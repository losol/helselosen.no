      <pageSimple id="1204" key="2043b5b4-c50a-4d94-8d1f-14ca7f075524" parentID="1087" level="3" creatorID="0" sortOrder="3" createDate="2016-02-28T16:58:50" updateDate="2016-02-28T16:58:50" nodeName="Hud og hår" urlName="hud-og-haar" path="-1,1052,1087,1204" isDoc="" nodeType="1085" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1084" nodeTypeAlias="pageSimple">
