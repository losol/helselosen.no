    <pageSection id="1185" key="23be30e2-2bbd-4c8d-9c0f-7a401a81806d" parentID="1052" level="2" creatorID="0" sortOrder="4" createDate="2016-02-24T21:55:12" updateDate="2016-03-01T22:41:16" nodeName="Graviditet" urlName="graviditet" path="-1,1052,1185" isDoc="" nodeType="1239" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1238" nodeTypeAlias="pageSection">
