      <pageSection id="1215" key="b2544609-8e3a-4819-baba-360cb1d9deb8" parentID="1125" level="3" creatorID="0" sortOrder="2" createDate="2016-02-28T17:13:27" updateDate="2016-03-02T14:05:18" nodeName="Taushetsplikt" urlName="taushetsplikt" path="-1,1052,1125,1215" isDoc="" nodeType="1239" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1238" nodeTypeAlias="pageSection">
