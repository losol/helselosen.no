      <pageStandard id="1134" key="608d1f2f-0b62-427e-b2bc-720b2540cf46" parentID="1119" level="3" creatorID="0" sortOrder="4" createDate="2016-02-23T18:53:16" updateDate="2016-03-14T23:46:41" nodeName="- Berøring er viktig for de små" urlName="beroering-er-viktig-for-de-smaa" path="-1,1052,1119,1134" isDoc="" nodeType="1241" creatorName="Ole Kristian Losvik" writerName="Ole Kristian Losvik" writerID="0" template="1246" nodeTypeAlias="pageStandard">
